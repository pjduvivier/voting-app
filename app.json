{
  "expo": {
    "name": "bmodelvoting-app",
    "owner":"thirdbrainsa-org",
    "slug": "bmodelvoting-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
     "android": {
      "package": "com.bmodelou.votingapp",
      "versionCode": 1  // also recommended for Play Store versioning
    }
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
